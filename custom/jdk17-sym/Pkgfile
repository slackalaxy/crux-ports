# Description: Creates a symlink to Adoptium 17 JDK
# URL:
# Maintainer: Petar Petrov, slackalaxy at gmail dot com
# Depends on: adoptium17-jdk

name=jdk17-sym
version=latest
release=1
source=()

build() {
	mkdir -p $PKG/usr/lib/jdk
	cd $PKG/usr/lib/jdk
	ln -s /usr/lib/jdk/adoptium17-jdk openjdk17-jdk
}
