# Description: Desktop wallpapers
# URL: https://github.com/slackalaxy/
# Maintainer: Petar Petrov, slackalaxy at gmail dot com

name=backgrounds-selected
version=0.08
release=1
source=(https://raw.githubusercontent.com/slackalaxy/sources/main/selected-$version.tar.xz)

build() {
	cd selected-$version
	
	mkdir -p $PKG/usr/share/backgrounds
	cp -a * $PKG/usr/share/backgrounds
}
