# Depends on: gnome-colors-48 gtk-xfce-engine gvfs orage pnmixer thunar-archive-plugin thunar-volman xdg-user-dirs xdg-utils xfce4-appfinder xfce4-clipman-plugin xfce4-cpufreq-plugin xfce4-dict xfce4-netload-plugin xfce4-notes-plugin xfce4-notifyd xfce4-places-plugin xfce4-power-manager xfce4-screenshooter xfce4-sensors-plugin xfce4-systemload-plugin xfce4-taskmanager xfce4-terminal xfce4-volumed-pulse xfce4-wavelan-plugin xfce4-weather-plugin xfce4-whiskermenu-plugin xfce4-xkb-plugin xfwm4-themes

name=xfce
version=4.12
release=1
source=()

build() {
	install -d -m 0755 $PKG/usr/etc
	echo "Xfce $version" > $PKG/usr/etc/xfce_version
}
