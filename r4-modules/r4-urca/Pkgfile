# Description: R module urca
# URL: https://cran.r-project.org/web/packages/urca
# Maintainer: Petar Petrov, slackalaxy at gmail dot com
# Depends on: r r4-nlme
# Optional: 

name=r4-urca
version=1.3.4
release=1
source=(https://cloud.r-project.org/src/contrib/urca_1.3-4.tar.gz)

build() {
	cd urca
	mkdir -p $PKG/usr/lib/R/library
	R CMD INSTALL . -l $PKG/usr/lib/R/library
}
