# Description: R module IRanges
# URL: https://bioconductor.org/packages/release/bioc/html/IRanges.html
# Maintainer: Petar Petrov, slackalaxy at gmail dot com
# Depends on: gcc-fortran r r4-s4vectors

name=r4-iranges
version=2.36.0
release=1
source=(https://bioconductor.org/packages/release/bioc/src/contrib/IRanges_${version}.tar.gz)

build() {
	cd IRanges
	mkdir -p $PKG/usr/lib/R/library
	R CMD INSTALL . -l $PKG/usr/lib/R/library
}
