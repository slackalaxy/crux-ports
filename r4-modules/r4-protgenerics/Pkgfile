# Description: R module ProtGenerics
# URL: https://bioconductor.org/packages/ProtGenerics
# Maintainer: Petar Petrov, slackalaxy at gmail dot com
# Depends on: r 
# Optional: r4-testthat

name=r4-protgenerics
version=1.42.0
release=1
source=(https://bioconductor.org/packages/3.22/bioc/src/contrib/ProtGenerics_${version}.tar.gz)

build() {
	cd ProtGenerics
	mkdir -p $PKG/usr/lib/R/library
	R CMD INSTALL . -l $PKG/usr/lib/R/library
}
