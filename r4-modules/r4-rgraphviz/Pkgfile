# Description: R module Rgraphviz
# URL: https://bioconductor.org/packages/Rgraphviz
# Maintainer: Petar Petrov, slackalaxy at gmail dot com
# Depends on: r r4-graph
# Optional: r4-biocgenerics r4-runit r4-xml

name=r4-rgraphviz
version=2.54.0
release=1
source=(https://bioconductor.org/packages/3.22/bioc/src/contrib/Rgraphviz_${version}.tar.gz)

build() {
	cd Rgraphviz
	mkdir -p $PKG/usr/lib/R/library
	R CMD INSTALL . -l $PKG/usr/lib/R/library
}
