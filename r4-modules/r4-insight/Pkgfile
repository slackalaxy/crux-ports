version=1.2.0
release=1
source=(https://cloud.r-project.org/src/contrib/insight_${version}.tar.gz)

build() {
	cd insight
	mkdir -p $PKG/usr/lib/R/library
	R CMD INSTALL . -l $PKG/usr/lib/R/library
}
