name=r4-lognormreg
version=0.5.0
release=1
source=(https://cloud.r-project.org/src/contrib/logNormReg_0.5-0.tar.gz)

build() {
	cd logNormReg
	mkdir -p $PKG/usr/lib/R/library
	R CMD INSTALL . -l $PKG/usr/lib/R/library
}
